                            <div class="card_image"> <img src="/resources/img/categorias/transport.jpg">
                                <div class="card_title title-white">
                                    <p>Transport</p>
                                    <% if(data.transport){ %>
                                        <p>
                                            <%= data.transport %> $
                                        </p>
                                        <% }else{ %>
                                            <p>0 $</p>
                                            <% } %>
                                </div>
                            </div>
